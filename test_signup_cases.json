      "actions": [
        {
          "type": "click_checkbox",
          "target": "terms"
        },
        {
          "type": "wait",
          "duration": 1
        },
        {
          "type": "check_element_state",
          "target": "signupBtn",
          "expected": "enabled"
        },
        {
          "type": "click_checkbox",
          "target": "terms"
        },
        {
          "type": "wait",
          "duration": 1
        },
        {
