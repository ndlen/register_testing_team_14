    {
      "id": "tc031",
      "description": "Signup button disabled when terms not checked, enabled when checked",
      "actions": [
        {
          "type": "check_element_state",
          "target": "signupBtn",
          "expected": "disabled"
        },
        {
          "type": "click_checkbox",
          "target": "terms"
        },
        {
          "type": "wait",
          "duration": 2
        },
        {
          "type": "check_element_state",
          "target": "signupBtn",
          "expected": "enabled"
        }
      ],
      "output": {}
    },
    {
      "id": "tc032",
